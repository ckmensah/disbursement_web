<%= simple_form_for(@payout_approval) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :payout_id %>
    <%= f.input :approver_code %>
    <%= f.input :approved %>
    <%= f.input :status %>
    <%= f.input :notified %>
    <%= f.input :level %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
