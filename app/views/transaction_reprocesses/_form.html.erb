<%= simple_form_for(@transaction_reprocess) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :old_trnx_id %>
    <%= f.input :new_trnx_id %>
    <%= f.input :amount %>
    <%= f.input :status %>
    <%= f.input :auto %>
    <%= f.input :err_code %>
    <%= f.input :user_id %>
    <%= f.input :nw_resp %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
